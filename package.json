{
  "name": "@fastify/one-line-logger",
  "version": "1.2.0",
  "description": "A simple formatter for fastify's logs",
  "main": "index.js",
  "types": "types/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "bin": {
    "one-line-logger": "./bin.js"
  },
  "dependencies": {
